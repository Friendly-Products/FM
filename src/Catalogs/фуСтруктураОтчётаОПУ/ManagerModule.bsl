Функция СоздатьЭлементСправочникаОПУ(СтруктураЗаполнения,Отказ)
	
	СправочникСсылка = Справочники.фуСтруктураОтчётаОПУ.ПустаяСсылка();
	
	Если Отказ Тогда
		Возврат СправочникСсылка;		
	КонецЕсли;
	
	СправочникСсылка = Справочники.фуСтруктураОтчётаОПУ.НайтиПоНаименованию(СтруктураЗаполнения.Наименование);
	Если ЗначениеЗаполнено(СправочникСсылка) Тогда
		СправочникОбъект = СправочникСсылка.ПолучитьОбъект();
	Иначе
		СправочникОбъект = Справочники.фуСтруктураОтчётаОПУ.СоздатьЭлемент();
	КонецЕсли;
	
	ЗаполнитьЗначенияСвойств(СправочникОбъект,СтруктураЗаполнения);
	СправочникОбъект.НеУдалятьСтрокуУровняВОтчете = Истина;
    
	Попытка
		СправочникОбъект.Записать();	
		СправочникСсылка = СправочникОбъект.Ссылка;	
	Исключение  
		Отказ = Истина;
		СправочникСсылка = Справочники.фуСтруктураОтчётаОПУ.ПустаяСсылка();
	    Сообщить("Ошибка при записи справочника. Описание: "+ОписаниеОшибки());
	КонецПопытки;
	
	Возврат СправочникСсылка;
		
